<h1>Listing song_listens</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @song_listens.each do |song_listen| %>
      <tr>
        <td><%= link_to 'Show', song_listen %></td>
        <td><%= link_to 'Edit', edit_song_listen_path(song_listen) %></td>
        <td><%= link_to 'Destroy', song_listen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song listen', new_song_listen_path %>
